{
  "name": "lasso-node",
  "version": "0.1.10",
  "description": "JavaScript widget and page unit testing with on the fly instrumentation",
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/lasso.git"
  },
  "keywords": [
    "testing",
    "unit test",
    "unit testing",
    "js",
    "javascript",
    "coverage",
    "code coverage",
    "istanbul",
    "phantomjs",
    "node"
  ],
  "dependencies": {
    "connect": "~2.11.0",
    "istanbul": "~0.1.44",
    "regexp-quote": "0.0.0",
    "optimist": "~0.6.0",
    "lodash": "~2.2.1",
    "untested": "~0.1.5"
  },
  "engines": {
    "node": ">= 0.8.0"
  },
  "scripts": {
    "test": "node lasso.js ./examples/basic/foo.html"
  },
  "pre-commit": "npm test",
  "pre-push": [
    "rm -rf node_modules",
    "npm install",
    "npm test"
  ],
  "license": "MIT",
  "preferGlobal": true,
  "main": "./lasso.js",
  "bin": "./lasso.js",
  "devDependencies": {
    "grunt-contrib-jshint": "~0.7.1",
    "grunt": "~0.4.1",
    "grunt-bump": "0.0.11",
    "grunt-nice-package": "~0.1.5",
    "matchdep": "~0.3.0",
    "pre-git": "0.0.13",
    "grunt-complexity": "~0.1.3"
  }
}
