{
  "name": "lasso-node",
  "version": "0.1.11",
  "description": "JavaScript widget and page unit testing with on the fly instrumentation",
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/lasso.git"
  },
  "keywords": [
    "testing",
    "unit test",
    "unit testing",
    "js",
    "javascript",
    "coverage",
    "code coverage",
    "istanbul",
    "phantomjs",
    "node"
  ],
  "dependencies": {
    "connect": "2.27.4",
    "istanbul": "0.3.2",
    "lodash": "~2.4.1",
    "optimist": "0.6.1",
    "regexp-quote": "0.0.0",
    "untested": "~0.1.5"
  },
  "engines": {
    "node": ">= 0.8.0"
  },
  "scripts": {
    "test": "node lasso.js ./examples/basic/foo.html"
  },
  "pre-commit": "npm test",
  "pre-push": [
    "rm -rf node_modules",
    "npm install",
    "npm test"
  ],
  "license": "MIT",
  "preferGlobal": true,
  "main": "./lasso.js",
  "bin": "./lasso.js",
  "devDependencies": {
    "grunt": "0.4.5",
    "grunt-bump": "0.0.16",
    "grunt-complexity": "0.2.0",
    "grunt-contrib-jshint": "0.10.0",
    "grunt-nice-package": "0.9.2",
    "matchdep": "~0.3.0",
    "pre-git": "0.1.1"
  }
}
